{
  "name": "game_prototype_network",
  "version": "0.1.0",
  "description": "isomorphic game server",
  "main": "dist/src/server/index.js",
  "scripts": {
    "test": "jest --config ./jest.config.js",
    "start": "node dist/server/index.js",
    "dev:start": "yarn dev && yarn start",
    "dev": "yarn build-css && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgYellow.bold,bgGreen.bold\" \"yarn dev-server\" \"yarn dev-client\"",
    "dev-server": "tsc-watch --noClear -b ./src/server/tsconfig.json --onSuccess \"node --enable-source-maps ./dist/server/index.js\"",
    "dev-client": "cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack serve",
    "build:start": "yarn build && yarn start",
    "build:server:start": "yarn build:server && yarn start",
    "build:client:start": "yarn build:client && yarn start",
    "build": "yarn build-css && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgYellow.bold,bgGreen.bold\" \"yarn build:server\" \"yarn build:client\"",
    "build-css": "postcss src/client/css/tailwind.css -o src/client/css/main.css",
    "build:server": "tsc -b src/server",
    "build:client": "cross-env NODE_ENV=production TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack --mode production"
  },
  "author": "Ernestas Monkevicius",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/plugin-proposal-numeric-separator": "^7.12.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.19",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/reduce-reducers": "^1.0.0",
    "@types/redux-actions": "^2.6.1",
    "@webpack-cli/serve": "^1.5.2",
    "autoprefixer": "^10.2.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^6.3.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "html-webpack-plugin": "^5.3.2",
    "immer": "^6.0.3",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "mini-css-extract-plugin": "^1.3.4",
    "phaser": "^3.52.0",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.3.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^4.2.0",
    "postcss-preset-env": "^6.7.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "tailwindcss": "^2.0.2",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.7",
    "ts-node": "^10.2.1",
    "tsc-watch": "^4.5.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.0.5",
    "webpack": "^5.18.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "tslib": "^2.3.1",
    "uWebSockets.js": "uNetworking/uWebSockets.js#v20.0.0"
  }
}
